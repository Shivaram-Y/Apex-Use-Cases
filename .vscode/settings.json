{
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.sfdx": true
  },
  "salesforcedx-vscode-core.show-cli-success-msg": false,
  "salesforce.einsteinForDevelopers.enable": true,
  "salesforce.einsteinForDevelopers.enableAutocompletions": true,
  "salesforce.einsteinForDevelopers.enableAutocompletionsByFileType": {
    "apex": true,
    "javascript": true,
    "css": true,
    "html": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "salesforce.einsteinForDevelopers.enableDebugLogging": true,
  "salesforce.einsteinForDevelopers.advanced": {},
  "codeAnalyzer.analyzeOnSave.enabled": false,
  "editor.formatOnPaste": true,
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "files.autoSave": "off",
  "files.autoSaveWhenNoErrors": false
}
